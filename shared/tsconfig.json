{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "outDir": "./dist",
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "isolatedModules": true,
    "composite": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "."
  },
  "include": ["src/**/*"],
  "exclude": ["dist", "node_modules"],
  "references": []
}
